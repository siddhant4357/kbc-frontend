{
  "routes": [
    { 
      "src": "^/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1" 
    },
    { 
      "src": "^/(.+\\.[a-zA-Z0-9]+)$", 
      "dest": "/$1" 
    },
    { "src": "^/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}